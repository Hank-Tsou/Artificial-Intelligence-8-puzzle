    "\n",
    "input_file = open('output.txt', 'r')\n",
    "output_file = open('h2_mycsv_EX.csv', 'w')\n",
    "\n",
    "for line in input_file:\n",
    "    (a, date, time, lon) = line.strip().split(' ')\n",
    "    output_file.write(','.join([a, date, time, lon]) + '\\n')\n",
    "input_file.close()\n",
    "output_file.close()"
